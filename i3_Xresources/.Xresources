! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: #000000, argb:ba000000, #000000
rofi.color-normal: argb:b5080808, argb:b5efefef, argb:b5080808, argb:b51e1e1e, argb:b2efefef
rofi.color-active: argb:bd080808, argb:bdefefef, argb:bd080808, argb:bd080808, argb:bdefefef
rofi.color-urgent: argb:c1080808, argb:beff615e, argb:b5080808, argb:b8dc322f, argb:b8efefef

! -------------------------------------------------------------------------------
! General Colors
! -------------------------------------------------------------------------------

! hard contrast: *background: #1d2021
*background: #010101
! soft contrast: *background: #32302f
*foreground: #f0f0f0
! Black + DarkGrey
*color0:  #282828
*color8:  #928374
! DarkRed + Red
*color1:  #cc241d
*color9:  #fb4934
! DarkGreen + Green
*color2:  #98971a
*color10: #b8bb26
! DarkYellow + Yellow
*color3:  #d79921
*color11: #fabd2f
! DarkBlue + Blue
*color4:  #458588
*color12: #83a598
! DarkMagenta + Magenta
*color5:  #b16286
*color13: #d3869b
! DarkCyan + Cyan
*color6:  #689d6a
*color14: #8ec07c
! LightGrey + White
*color7:  #a89984
*color15: #ebdbb2
